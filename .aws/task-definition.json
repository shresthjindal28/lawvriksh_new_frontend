{
  "family": "frontend-task",
  "containerDefinitions": [
    {
      "name": "ui-container",
      "image": "331005568033.dkr.ecr.ap-south-1.amazonaws.com/lawvriksh/frontend:latest",
      "cpu": 512,
      "memory": 1536,
      "memoryReservation": 1024,
      "portMappings": [
        {
          "name": "ui-port",
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "NEXT_PUBLIC_API_BASE_URL",
          "value": "https://app.lawvriksh.com/api/"
        },
        {
          "name": "NEXT_PUBLIC_TASK_WS_URL",
          "value": "wss://app.lawvriksh.com/api/ws/tasks"
        },
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "PORT",
          "value": "3000"
        },
        {
          "name": "NEXT_PUBLIC_WS_URL_GRAMMER_SPELL",
          "value": "wss://app.lawvriksh.com/api/ai/ws/spell-grammer/check"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "secrets": [
        {
          "name": "DEEPGRAM_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:331005568033:secret:lawvriksh/env-secrets-jc44mJ:DEEPGRAM_API_KEY::"
        },
        {
          "name": "NEXT_PUBLIC_GMAIL_CLIENT_ID",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:331005568033:secret:lawvriksh/env-secrets-jc44mJ:NEXT_PUBLIC_GMAIL_CLIENT_ID::"
        },
        {
          "name": "GMAIL_CLIENT_SECRET",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:331005568033:secret:lawvriksh/env-secrets-jc44mJ:GMAIL_CLIENT_SECRET::"
        },
        {
          "name": "GMAIL_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:331005568033:secret:lawvriksh/env-secrets-jc44mJ:GMAIL_API_KEY::"
        },
        {
          "name": "TIPTAP_PRO_TOKEN",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:331005568033:secret:lawvriksh/env-secrets-jc44mJ:TIPTAP_PRO_TOKEN::"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/frontend-task",
          "awslogs-create-group": "true",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "systemControls": []
    }
  ],
  "taskRoleArn": "arn:aws:iam::331005568033:role/lv-ecs-task-role",
  "executionRoleArn": "arn:aws:iam::331005568033:role/lv-ecs-task-execution-role",
  "networkMode": "bridge",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": ["EC2"],
  "cpu": "512",
  "memory": "1536",
  "runtimePlatform": {
    "cpuArchitecture": "ARM64",
    "operatingSystemFamily": "LINUX"
  },
  "enableFaultInjection": false
}
